<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Message" table="messages">
		<id name="messageid" type="int">
			<column name="messageid" />
			<generator class="assigned" />
		</id>
		<property name="sender" type="java.lang.String">
			<column name="sender" />
		</property>
		<property name="accepter" type="java.lang.String">
			<column name="accepter" />
		</property>
		<property name="role" type="java.lang.String">
			<column name="role" />
		</property>
		<property name="content" type="java.lang.String">
			<column name="content" />
		</property>
		<property name="time" type="java.sql.Timestamp">
			<column name="time" />
		</property>

	</class>
</hibernate-mapping>